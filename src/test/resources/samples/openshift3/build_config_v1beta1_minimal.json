{
    "apiVersion": "v1beta1",
    "kind": "BuildConfig",
    "metadata": {
        "namespace": "hello-openshift-project",
        "name": "javaparks",
    },
    "triggers": [
        {
            "type": "github",
            "github": {
                "secret": "secret101"
            }
        },
        {
            "type": "generic",
            "generic": {
                "secret": "secret101"
            }
        }
    ],
    "parameters": {
        "source": {
            "type": "Git",
            "git": {
                "uri": "git@github.com:jcantrill/javaparks.git"
            }
        },
        "strategy": {
            "type": "STI",
            "stiStrategy": {
                "image": "openshift/wildfly-8-centos:latest"
            }
        },
        "output": {
            "imageTag": "jcantrill/javaparks:latest",
            "registry": "172.30.17.59:5001"
        }
    },
    "labels" : {
        "name" : "javaparks"
    }
    
}
